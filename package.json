{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "gulp": "gulp --gulpfile gulpfile.js",
    "package": "electron-packager . --platform=darwin --arch=x64 --electron-version=1.7.8 --overwrite --ignore=node_modules --ignore=.gitignore",
    "package-win": "electron-packager . --platform=win32 --arch=ia32 --electron-version=1.7.8 --overwrite --ignore=node_modules --ignore=.gitignore",
    "package-installer": "node ./script/installer.js",
    "asar": "asar pack electron-quick-start-darwin-x64/electron-quick-start.app/Contents/Resources/app electron-quick-start-darwin-x64/electron-quick-start.app/Contents/Resources/app.asar"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "asar": "^0.14.0",
    "electron": "~1.7.8",
    "electron-packager": "^9.1.0"
  },
  "dependencies": {
    "angular-material": "^1.1.5",
    "electron-winstaller": "^2.6.3",
    "gulp": "^3.9.1",
    "gulp-notify": "^3.0.0",
    "gulp-sass": "^3.1.0",
    "node-xlsx": "^0.11.0"
  }
}
